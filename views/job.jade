extends layout

block body
	div.container
		div.row
			div.span12
				div.navbar
					div.navbar-inner
						div.container
							a.brand Champagne
							div#nav-collapse01.nav-collapse.collapse
								ul.nav
									li.active 
										a(href='/admin/') Websites
									li
										a Users
		div.row
			div.span12
				h3
					img(src="/images/icons/Infinity-Loop@2x.png",style="display: inline-block;max-height: 30px;max-width: 30px;vertical-align: baseline;")
					|&nbsp;
					=job.type 
					|&nbsp;is 
					=job.status	
					
				table.table.table-striped.smallTable
					thead
						tr
							th Timestamp
							th Message
							th Level
							th URL
					tbdody
						- each log in logs
							tr
								td
									=log.createdAt
								td	
									=log.message
								td	
									=log.level
								td
									a(href=log.url) #{log.url}					
												
						
	script(src='/js/ui/jquery-1.8.3.min.js')
	script(src='/js/ui/jquery-ui-1.10.3.custom.min.js')
	script(src='/js/ui/jquery.ui.touch-punch.min.js')
	script(src='/js/ui/bootstrap.min.js')
	script(src='/js/ui/bootstrap-select.js')
	script(src='/js/ui/bootstrap-switch.js')
	script(src='/js/ui/flatui-checkbox.js')
	script(src='/js/ui/flatui-radio.js')
	script(src='/js/ui/jquery.tagsinput.js')
	script(src='/js/ui/jquery.placeholder.js')
	script(src='/js/ui/jquery.stacktable.js')
	script(src='/js/app.js')

