!!!
html(lang='fr')
	-var menu = {'Search': {url:'/', text:'Recherchez et comparez par producteur, gamme de prix...'},'Big data': {url:'/bigdata/', text:'Les données décortiquées de la vente en ligne de champagne'}, 'Blog': {url:'/blog/', text: 'Nos plus belles infos sur les plus belles bulles'} };
	block menu
		-var selected = 'Search';
	head
		meta(charset='utf8')
		meta(name='viewport' content='width=device-width, initial-scale=1.0')
		base(href='/')
		title La Cote des Bulles
		block css
			link(rel='stylesheet', href='/bootstrap/css/bootstrap.css')
			link(rel='stylesheet', href='/css/flat-ui.css')
			link(rel='stylesheet', href='/css/ui.slider.extras.css')
			link(rel='stylesheet', href='/css/app.css')
	
	include seo
	body
		#wrap
			nav
				.container
					#brand
						h1 
							a(href="/") la cote des bulles
						h2 Le comparateur indépendant des prix du champagne
					#spinner	
					ul.hidden-xs
						each val, key in menu
							li
								if selected === key
									a.active(href=val.url, title=key)= key
									div 
										=val.text
								else
									a(href=val.url, title=key)= key
									div 
										=val.text
			block header							
			block content
		#footer
			include footer

		block scripts
			script(src='/js/ui/jquery-1.8.3.min.js')
			script(src='/js/ui/jquery-ui-1.10.3.custom.min.js')
			script(src='/js/ui/jquery.ui.touch-punch.min.js')
			script(src='/js/ui/jquery.ui.selecttoslider.js')
			
			script(src='/js/ui/bootstrap.min.js')
			script(src='/js/ui/bootstrap-select.js')
			script(src='/js/ui/bootstrap-switch.js')
			script(src='/js/ui/flatui-checkbox.js')
			script(src='/js/ui/flatui-radio.js')
			script(src='/js/ui/jquery.tagsinput.js')
			script(src='/js/ui/jquery.placeholder.js')
			
			script(src='/js/plugins/spin.min.js')
			script(src='/js/plugins/jquery.spin.js')
			
			script(src='/js/presentation.js')

			
			script.
				(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
				(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
				m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
				})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

				ga('create', 'UA-42238568-1', 'lacotedesbulles.com');
				ga('send', 'pageview');